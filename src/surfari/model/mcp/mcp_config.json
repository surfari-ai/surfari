{
    "servers": {
        "http_filesystem": {
            "embedded_http": true,
            "root": "file_sandbox"
        },
        "stdio_filesystem": {
            "disabled": true,
            "command": "python3",
            "args": [
                "-m",
                "surfari.model.mcp.fs_server",
                "."
            ],
            "cwd": ".",
            "env": {}
        },
        "everything": {
            "disabled": true,
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-everything"
            ]
        }
    }
}